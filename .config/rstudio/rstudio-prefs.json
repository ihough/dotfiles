{
    "always_save_history": false,
    "auto_append_newline": true,
    "auto_detect_indentation": true,
    "check_arguments_to_r_function_calls": true,
    "check_unexpected_assignment_in_function_call": true,
    "code_completion": "triggered",
    "code_completion_other": "triggered",
    "default_encoding": "UTF-8",
    "default_project_location": "/Users/houghi/code",
    "editor_theme": "Monokai",
    "full_project_path_in_window_title": true,
    "highlight_r_function_calls": true,
    "highlight_selected_line": true,
    "initial_working_directory": "~/CloudUGA/code/misc",
    "jobs_tab_visibility": "shown",
    "knit_working_dir": "project",
    "line_ending_conversion": "posix",
    "margin_column": 100,
    "new_proj_git_init": true,
    "new_proj_use_renv": true,
    "panes": {
        "quadrants": [
            "Source",
            "TabSet1",
            "Console",
            "TabSet2"
        ],
        "tabSet1": [
            "History",
            "Files",
            "Packages",
            "VCS",
            "Presentation"
        ],
        "tabSet2": [
            "Environment",
            "Plots",
            "Help",
            "Viewer",
            "Presentations"
        ],
        "hiddenTabSet": [
            "Connections",
            "Build",
            "Tutorial"
        ],
        "console_left_on_top": false,
        "console_right_on_top": true,
        "additional_source_columns": 0
    },
    "posix_terminal_shell": "zsh",
    "rainbow_parentheses": true,
    "rmd_auto_date": true,
    "rmd_viewer_type": "pane",
    "save_workspace": "never",
    "show_doc_outline_rmd": true,
    "show_invisibles": true,
    "source_with_echo": true,
    "strip_trailing_whitespace": true,
    "style_diagnostics": true,
    "syntax_color_console": true,
    "terminal_bell_style": "none",
    "visual_markdown_editing_show_margin": true,
    "warn_if_no_such_variable_in_scope": true,
    "warn_variable_defined_but_not_used": true
}
